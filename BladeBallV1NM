local Others = MakeTab({Name = "Others"})

--[[
  Name = "Main" <string> Nome da guia
]]


local Players = game:GetService("Players")
local player = Players.LocalPlayer
local kills = 0

-- Crear el label
local Label = AddTextLabel(Others, tostring(kills))

-- Función para actualizar las kills
local function onKill()
    kills = kills + 1
    Label.Text = tostring(kills)
end

-- Conectar el evento cuando matas a alguien
-- Aquí debes reemplazar "KillEvent" con el evento correcto que se dispare cuando consigas una kill en Bladeball
player.KillEvent.OnClientEvent:Connect(onKill)
