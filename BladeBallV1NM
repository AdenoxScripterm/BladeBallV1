local toggleScript

local Toggle = AddToggle(Main, {
    Name = "Toggle teste",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Ejecutar el loadstring y almacenar el resultado
            toggleScript = loadstring(game:HttpGet("https://raw.githubusercontent.com/AdenoxScripterm/AUTODETECTSPAMV4-WITHV3-/main/SPAMV4"))()
        else
            -- Verificar si el script se ha cargado y llamarlo para detenerse
            if toggleScript and toggleScript.StopScript then
                toggleScript.StopScript()
            end
        end
    end
})



AddButton(Parry, {
  Name = "ParryV2",
